#include <iostream>
#include<cstdlib>


using namespace std;

int n, t;

int main ( )
{
    int n, t;
     cin >> t;

    while (t--) {

    cin>>n;

    if ( n<0 ) cout<< n <<" is negative number ";
    if ( n>0 ) cout<< n <<" is positive number ";
    else if  ( n==0 ) cout<<" n is zero ";
    cout<<endl;
    }

    return 0 ;
}
